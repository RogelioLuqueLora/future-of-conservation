<div class="row">
  <div class="col-lg-6">
    <div class="card">
      <div class="card-block">
        <h4 class="card-title">See how you compare with others</h4>
        <p class="card-text">Comparing your results against <%= @all_other_results.count %> other <%= "result".pluralize(@all_other_results.count) %></p>
      </div>
      <%= scatter_chart @data, @config %>
    </div>
  </div>

  <div class="col-lg-6">
    <div class="card">
      <div class="card-block">
        <h4 class="card-title">Your results</h4>
        <p class="card-text">This information is for testing only</p>
      </div>
      <ul class="list-group list-group-flush">
        <li class="list-group-item">
          <span class="text-primary">IP Address: </span>
          <%= @results.ip_address %>
        </li>
        <li class="list-group-item">
          <span class="text-primary">X Axis Total: </span>
          <%= @results.x_axis_total%>
        </li>
        <li class="list-group-item">
          <span class="text-primary">Y Axis Total: </span>
          <%= @results.y_axis_total%>
        </li>
        <li class="list-group-item">
          <span class="text-primary">X Axis (Scaled): </span>
          <%= @results.x_axis_scaled%>
        </li>
        <li class="list-group-item">
          <span class="text-primary">Y Axis (Scaled): </span>
          <%= @results.y_axis_scaled%>
        </li>
        <li class="list-group-item">
          <span class="text-primary">Date Submitted: </span>
          <%= @results.created_at.to_formatted_s(:long)%>
        </li>
      </ul>
    </div>

    <div id="accordion" role="tablist" aria-multiselectable="true">
      <div class="card">
        <div class="card-header" role="tab" id="headingOne">
          <h5 class="mb-0">
            <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
              Filter results by Gender
           </a>
          </h5>
        </div>

        <div id="collapseOne" class="collapse in" role="tabpanel" aria-labelledby="headingOne">
          <div class="card-block">
            <%= form_tag results_path(@results.uuid), method: :get do %>
              <p>
                <%= radio_button_tag "filter[gender]", 'Male' %>
                <%= label_tag "filter[gender]", 'Show only results by Males' %>
              </p>
              <p>
                <%= radio_button_tag "filter[gender]", 'Female' %>
                <%= label_tag "filter[gender]", 'Show only results by Females' %>
              </p>

              <%= submit_tag 'Filter', class: 'btn btn-primary' %>
            <% end %>
          </div>
        </div>
      </div>
